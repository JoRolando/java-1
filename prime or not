import java.util.*;
public class prime{
	public static void main(String[] args){
		int d=2;
		Scanner in = new Scanner(System.in);
		System.out.println("Enter n : ");
		int n = in.nextInt();
		System.out.println("Enter n : ");
		while (d<n){
			if (n%d==0){
				break;
			}
		d++;	
		}
		if (d==n){
			System.out.println("Given number is prime number ! ");
		}
		else
			System.out.println("Given number is not a prime number ! ");
	}	
}
